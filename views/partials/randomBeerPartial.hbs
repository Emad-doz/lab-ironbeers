<div >
  <div class="row no-gutters">
    <div class="col-md-4">
      <img src="{{this.image_url}}" class="card-img" alt="{{this.name}}">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title"><b>{{this.name}}</b></h5>
        <p class="card-text"><small class="text-muteTd">{{this.tagline}}</small></p>
        <p class="card-text">{{this.description}}</p>
        <p><h3>Food Pairing:</h3><p>
          <ul>
            {{#each this.food_pairing}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
        <p><h3>Brewers Tips:</h3>{{this.brewers_tips}}</p>
      </div>
    </div>
  </div>
</div>